export MYSQL="/usr/local/.mysql/bin"
export PSQL="$HOME/.postgres/pg10/bin"
export CONDA="$HOME/.conda/bin"
export HOME_BIN="$HOME/bin"
export SPARK="$HOME/.spark/bin"

if [ "${KERNEL:0:6}" = "Darwin" ]; then
  export PATH="$CONDA:$HOME_BIN:$MYSQL:$PSQL:$PATH"
elif [ "${KERNEL:0:5}" = "Linux" ]; then
  export PYTHONPATH="$SPARK/python:$PYTHONPATH"
  export PATH="$SPARK:$CONDA:$HOME_BIN:$PATH"
fi
